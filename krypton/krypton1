ssh krypton1@krypton.labs.overthewire.org -p 2222 

cd /krypton/krypton1/
cat krypton2

# YRIRY GJB CNFFJBEQ EBGGRA

# CEASAR CIPHER 
# Assuming the second word is THE and the third is PASSWORD
# Its G-T -> 7-20 = 13

echo 'YRIRY GJB CNFFJBEQ EBGGRA' | tr '[A-Z]' '[N-ZA-M]'

# or

./ceasar.py --input "YRIRY GJB CNFFJBEQ EBGGRA" --num 13

# LEVEL TWO PASSWORD ROTTEN
> ROTTEN
